        <activity
            android:name=".LogIn"
            android:exported="true"
            android:theme="@style/Theme.DateVenture.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignIn"
            android:exported="true"
            android:label="signin"
